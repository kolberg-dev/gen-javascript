{{define "types"}}

{{- $opts := .Opts -}}

//
// Types
//
{{ if .Messages -}}
{{range .Messages -}}

{{if .Type | isEnum -}}
{{$enumName := .Name}}
{{if $opts.Export}}export {{end}}var {{$enumName}};
(function ({{$enumName}}) {
{{- range $i, $field := .Fields}}
  {{$enumName}}["{{$field.Name}}"] = "{{$field.Name}}"
{{- end}}
})({{$enumName}} || ({{$enumName}} = {}))
{{end -}}

{{- if .Type | isStruct  }}
{{if $opts.Export}}export {{end}}class {{.Name}} {
  constructor(_data) {
    this._data = {}
    if (_data) {
      {{range .Fields -}}
      this._data['{{. | jsExportedJSONField}}'] = _data['{{. | jsExportedJSONField}}']
      {{end}}
    }
  }
  {{ range .Fields -}}
  get {{. | jsExportedJSONField}}() {
    return this._data['{{. | jsExportedJSONField }}']
  }
  set {{. | jsExportedJSONField}}(value) {
    this._data['{{. | jsExportedJSONField}}'] = value
  }
  {{end}}
  toJSON() {
    return this._data
  }
}
{{end -}}
{{end -}}
{{end -}}

{{end}}
